<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="DbConnection" connectionString="data.db;" />
  </connectionStrings>
  <appSettings>
    <add key="ServicePort" value="8080" />
    <!-- 文字动画效果的速度 -->
    <add key="AnimationSpeed" value="5" />
    <!--播放器休息查询间隔时间-->
    <add key="Interval" value="15"/>
  </appSettings>
</configuration>