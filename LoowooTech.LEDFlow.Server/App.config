<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="ServicePort" value="8080" />
    <!-- 文字动画效果的速度 0(最快) 20(最慢) -->
    <add key="AnimationSpeed" value="5" />
    <!-- 文字动画效果速度2 20(最快) 200(最慢)-->
    <add key="FrameTime" value="200"/>
    <!--播放节目间隔时间-->
    <add key="Interval" value="5"/>
  </appSettings>
</configuration>